<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.midlandroid.apps.android.timerwithsetcounter"
      android:versionCode="1" android:versionName="1.0.9">
    <application android:label="@string/app_name"
    		android:description="@string/app_desc"
    		android:testOnly="false"
    		android:icon="@drawable/timer_icon">
    		
        <activity android:name=".Main"
                  android:label="@string/app_name"
                  android:launchMode="singleInstance">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <service android:name=".timerservice.TimerService"/>
    	<activity android:name=".Preferences"
				android:label="@string/preferences"
    			android:launchMode="singleInstance" />
	<activity android:label="@string/timer_delay_countdown_label" android:name="DelayTimeCountDown"></activity>
</application>
    <uses-sdk android:minSdkVersion="4" />

<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
</manifest> 